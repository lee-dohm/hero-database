{
    "plugins": [
      "node_modules/jsdoc-babel",
      "./doc-plugins/hero",
      "./doc-plugins/markdown"
    ],
    "templates": {
      "theme": "cerulean",
      "systemName": "Hero Database"
    },
    "opts": {
      "destination": "./docs/",
      "template": "./node_modules/ink-docstrap/template",
      "readme": "README.md",
      "recurse": true
    },
    "markdown": {
      "smart": true
    }
}
